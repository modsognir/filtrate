<p><%= params.inspect %></p>
<table>
  <tr>
    <th>Name</th>
  </tr>
  <% @filtered.each do |record| %>
    <tr>
      <td><%= record.name %></td>
    </tr>
  <% end %>
<table>
  
<%= filtrate users_path do |f|%>
  <%= f.filter :name %>
  <%= f.filter_date :date_of_birth, :prefix => "Date of Birth" %>
  <%= f.submit "Submit" %>
<% end %>